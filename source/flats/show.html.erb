<!-- source/flats/show.html.erb -->

<!-- Access data.flats with owner's name as key -->
<% flat = data.flats[owner] %>

<!-- Inject flat data in template -->

<%= image_tag "https://kitt.lewagon.com/placeholder/cities/#{flat.city_slug}", class: "banner" %>
<%= image_tag "https://kitt.lewagon.com/placeholder/users/#{owner}", class: "navbar_wagon avatar" %>

<p><%= flat.name %></p>
<h1><%= flat.description %></h1>
<p><%= flat.city_name %></p>






